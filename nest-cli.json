{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/tanager/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/tanager/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/tanager",
  "projects": {
    "tanager": {
      "type": "application",
      "root": "apps/tanager",
      "entryFile": "main",
      "sourceRoot": "apps/tanager/src",
      "compilerOptions": {
        "tsConfigPath": "apps/tanager/tsconfig.app.json"
      }
    },
    "inbound": {
      "type": "application",
      "root": "apps/inbound",
      "entryFile": "main",
      "sourceRoot": "apps/inbound/src",
      "compilerOptions": {
        "tsConfigPath": "apps/inbound/tsconfig.app.json"
      }
    },
    "commons": {
      "type": "library",
      "root": "libs/commons",
      "entryFile": "index",
      "sourceRoot": "libs/commons/src",
      "compilerOptions": {
        "tsConfigPath": "libs/commons/tsconfig.lib.json"
      }
    },
    "exceptions": {
      "type": "library",
      "root": "libs/exceptions",
      "entryFile": "index",
      "sourceRoot": "libs/exceptions/src",
      "compilerOptions": {
        "tsConfigPath": "libs/exceptions/tsconfig.lib.json"
      }
    },
    "outbound": {
      "type": "application",
      "root": "apps/outbound",
      "entryFile": "main",
      "sourceRoot": "apps/outbound/src",
      "compilerOptions": {
        "tsConfigPath": "apps/outbound/tsconfig.app.json"
      }
    },
    "interceptors": {
      "type": "library",
      "root": "libs/interceptors",
      "entryFile": "index",
      "sourceRoot": "libs/interceptors/src",
      "compilerOptions": {
        "tsConfigPath": "libs/interceptors/tsconfig.lib.json"
      }
    },
    "models": {
      "type": "library",
      "root": "libs/models",
      "entryFile": "index",
      "sourceRoot": "libs/models/src",
      "compilerOptions": {
        "tsConfigPath": "libs/models/tsconfig.lib.json"
      }
    }
  }
}
